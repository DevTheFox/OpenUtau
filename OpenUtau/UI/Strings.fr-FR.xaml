    <system:String x:Key="errors.caption">Erreur</system:String>
    <system:String x:Key="warning">Attention</system:String>
    <system:String x:Key="warning.asciipath">Le chemin d'installation d'OpenUTAU "{0}" contient des caractères non-ASCII. Les resampler en .EXE pourraient ne pas marcher.</system:String>

